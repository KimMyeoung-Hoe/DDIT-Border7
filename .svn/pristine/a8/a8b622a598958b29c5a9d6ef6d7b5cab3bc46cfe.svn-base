/* 고객지원 탭 메뉴 스타일                   */
/* ========================================================= */
.support-tabs {
    display: flex;
    margin-bottom: 30px;
    border-bottom: 1px solid #4a4a60; /* 하단 경계선 */
}

.support-tabs .tab-item {
    padding: 0 15px 25px 15px;
    color: #a0a0d0;
    font-size: 1.1em;
    font-weight: bold;
    cursor: pointer;
    position: relative; /* 하단 활성화 라인 */
    transition: color 0.3s;
}

.support-tabs .tab-item:hover {
    color: #000000;
}

.support-tabs .tab-item.active {
    color: #000000;
}

.support-tabs .tab-item.active::after {
    content: '';
    position: absolute;
    bottom: -1px; /* 부모 border-bottom 위로 */
    left: 0;
    width: 100%;
    height: 3px;
    background-color: #000000;
    border-radius: 2px;
}

.support-tabs .tab-icon {
    margin-right: 8px;
    font-size: 1.2em;
}


/*진행 상태 엘리먼트 css*/
.progress-section {
}

.progress-title {
	font-size: 18px;
	font-weight: bold;
	margin-bottom: 15px;
	color: #333;
}

.progress-bar-container {
	background-color: #f8f9fa;
	padding: 20px;
	border-radius: 8px;
	margin-bottom: 10px;
}

.progress-bar {
	display: flex;
	align-items: center;
	justify-content: space-between;
	flex-direction: row;
	position: relative;
	overflow: hidden;
	background: transparent;
}

.progress-step {
	display: flex;
	flex-direction: column;
	align-items: center;
	flex: 1;
	position: relative;
}

.progress-details {
	position: absolute;
	top: 70px;
	left: 0;
}

.progress-step:not(:last-child)::after {
	content: '';
	position: absolute;
	top: 15px;
	right: -50%;
	width: 100%;
	height: 2px;
	background-color: #007bff;
	z-index: 1;
}

.step-circle {
	width: 30px;
	height: 30px;
	border-radius: 50%;
	background-color: #007bff;
    border: 2px solid #007bff;
	color: white;
	display: flex;
	align-items: center;
	justify-content: center;
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 8px;
	z-index: 2;
	position: relative;
}

.step-circle.inactive {
	background-color: #fff;
	color: #007bff;
}

.step-circle.wait {
	background-color: yellow;
	color: #6c757d;
}

.progress-date {
	font-size: 12px;
	color: #6c757d;
	margin-top: 5px;
}

.card-main{
	display: flex;
    justify-content: space-between;
}
.card-left{
    width: 48%;
}
.card-right{
	width: 48%;
} 
