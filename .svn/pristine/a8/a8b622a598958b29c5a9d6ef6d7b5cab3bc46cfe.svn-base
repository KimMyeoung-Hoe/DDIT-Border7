/* 탭 컨테이너 */
.tabs-container {
	width: 100%;
	margin: 20px 0;
	border: 1px solid #ddd;
	border-radius: 5px;
	overflow: hidden; /* 내용이 넘칠 경우 스크롤 처리 */
	/* 탭 헤더 (버튼 부분) */
}
.tabs-header { display : flex; /* 버튼들을 가로로 나열 */
	background-color: #f1f1f1;
	border-bottom: 1px solid #ddd;
}

/* 각 탭 버튼 */
.tab-button {
	padding: 15px 20px;
	cursor: pointer;
	text-align: center;
	flex-grow: 1; /* 버튼들이 균등하게 공간을 차지하도록 */
	transition: background-color 0.3s ease; /* 호버 효과 */
	border-right: 1px solid #eee; /* 버튼 구분선 */
}

.tab-button:last-child {
	border-right: none; /* 마지막 버튼은 오른쪽 선 제거 */
}

.tab-button:hover {
	background-color: #e9e9e9;
}

/* 활성화된 탭 버튼 */
.tab-button.active {
	background-color: #fff;
	border-bottom: 2px solid #007bff; /* 활성화된 탭 하단에 강조선 */
	color: #007bff;
	font-weight: bold;
}

/* 탭 내용 */
.tabs-content {
	padding: 20px;
}
.tabs-content-2nd:has(.tabs-content + .tabs-content){
	display: flex;
}
.tabs-content-2nd:has(.tabs-content + .tabs-content) .tabs-content{
	width: 50%;
}
.tabs-content-2nd:has(.tabs-content + .tabs-content) .tabs-content:first-child{
	width: 70%;
	padding-right: 10px;
}
.tabs-content-2nd:has(.tabs-content + .tabs-content) .tabs-content:last-child{
	width: 30%;
	padding-left: 10px;
}
/* 각 탭 패널 (내용 부분) */
.tab-pane {
	display: none; /* 기본적으로 숨김 */
}

/* 활성화된 탭 패널 */
.tab-pane.active {
	display: block; /* 활성화되면 보임 */
}
.contract-progress-details{
	font-size: 18px;
	color: #333
}
/* 진행과정 탭 */
.progress-history-container {
	padding-left: 20px; /* 아이콘과 세로 선을 위한 왼쪽 여백 */
	position: relative;
	background-color: #f8f9fa; /* 배경색 통일 */
	padding: 25px; /* 전체 패딩 */
	border-radius: 8px;
	border: 1px solid #e9ecef; /* 테두리 */
}

/* 각 진행 단계 아이템 */
.progress-history-item {
	display: flex; /* 아이콘과 내용을 가로로 배열 */
	align-items: flex-start; /* 세로 정렬: 시작점(상단)에 맞춤 */
	margin-bottom: 25px; /* 각 아이템 간의 간격 */
	position: relative;
	z-index: 1; /* 아이템이 선 위에 오도록 */
}

/* 마지막 아이템의 하단 간격 조정 */
.progress-history-item:last-child {
	margin-bottom: 0;
}

/* 각 단계의 동그란 아이콘 */
.status-icon {
	flex-shrink: 0; /* 아이콘 크기 고정, 내용이 줄어들어도 아이콘은 그대로 */
	width: 28px; /* 아이콘 원형의 너비 */
	height: 28px; /* 아이콘 원형의 높이 */
	border-radius: 50%; /* 원형으로 만들기 */
	background-color: #007bff; /* 파란색 배경 */
	color: white; /* 아이콘 색상 */
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 15px; /* Font Awesome 아이콘 크기 */
	margin-right: 15px; /* 아이콘과 텍스트 간격 */
	position: relative;
	z-index: 2; /* 아이콘이 세로 선 위에 오도록 */
}

/* Font Awesome 아이콘 크기 미세 조정 */
.status-icon .fas {
	font-size: 0.9em; /* 아이콘 폰트 크기 조절 */
}

/* 진행 단계의 내용 (이름, 날짜, 설명) */
.status-content {
	flex-grow: 1; /* 남은 공간을 모두 차지 */
}

/* 단계 이름 (예: 관세계약 접수) */
.status-name {
	font-weight: bold;
	color: #333;
	font-size: 16px;
	margin-bottom: 3px; /* 날짜와의 간격 */
}

/* 날짜와 시간 (예: 2025-05-10 09:00) */
.status-date {
	font-size: 13px;
	color: #6c757d;
	margin-bottom: 5px; /* 설명과의 간격 */
}

/* 추가 설명 텍스트 (예: 의뢰 접수 완료) - 이 부분은 데이터에 따라 조정 필요 */
.status-description {
	font-size: 14px;
	color: #555;
	line-height: 1.5;
}

/* 각 진행 단계 아이템을 연결하는 세로 선 */
.vertical-line {
	position: absolute;
	top: 28px; /* 아이콘의 대략적인 중앙(높이/2 + 아이템 상단 패딩 고려)에서 시작 */
	left: 34px;
	/* 아이콘의 중앙(status-icon width/2 + container padding-left)에 오도록 조정 */
	width: 2px; /* 선의 두께 */
	height: calc(100% - 28px); /* 아이템 높이에서 아이콘 위쪽까지의 길이 제외 */
	background-color: #e9ecef; /* 선 색상 (연한 회색) */
	z-index: 0; /* 내용 뒤로 배치 */
}

/* 마지막 진행 단계 아이템에는 세로 선을 숨김 */
.progress-history-item:last-child .vertical-line {
	display: none;
}

@media ( max-width : 768px) {
	.content-section {
		grid-template-columns: 1fr;
		gap: 20px;
	}
	.action-buttons {
		flex-direction: column;
		gap: 10px;
	}
	.progress-bar {
		flex-direction: column;
		gap: 20px;
	}
	.progress-step:not(:last-child)::after {
		display: none;
	}
}
.seal-wrap {
    display: none;
	position: absolute;
	right: 50px;
	bottom: 140px;
}
.seal-wrap.stamp{
    display: block;
	animation-name: stamp;
	animation-duration: 0.5s;
	animation-iteration-count: 1;
	animation-timing-function: ease;
}
@keyframes stamp {
	0% {transform: scale(1); opacity:0;}
	95% {transform: scale(5); opacity:0;}
	100% {transform: scale(1); opacity:1;}
}

