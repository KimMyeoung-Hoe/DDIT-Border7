/* 롤링 배너 컨테이너 */
.rollingBanner {
    flex-grow: 1;
    overflow: hidden;
    white-space: nowrap;
    position: relative; 
    height: 30px;
    display: flex;
    align-items: center;
    margin: 0 30px;
}

/* 롤링될 텍스트 */
.rolling-text {
    display: inline-block;
    width:100%;
    position: absolute; 
    left: 0%;
    font-size: 30px;
    color: #ffffff;
    font-weight: bold;
    margin:0;
    animation: roll 16s linear infinite;
}

/* 롤링 애니메이션 정의 */
@keyframes roll {
    0% {
        transform: translateX(0%);
    }
    50%{
        transform: translateX(90%);
    }
    100%{
    	transform: translateX(0%) ;
    }
}
