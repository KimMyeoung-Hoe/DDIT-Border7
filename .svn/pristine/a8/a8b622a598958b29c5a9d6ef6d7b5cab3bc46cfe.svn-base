body {
	margin: 0;
	padding:0;
}

/* 메인영역 상단 밀기 및 배경 */
.app-container {
  display: flex;
  flex-grow: 1;
  /* background: radial-gradient(farthest-side ellipse at 10% 0, #333867 20%, #17193b); */
  /* background: #ffffff; */
  background: #f2f4fA;
  color: #00000000;
  margin-top: 60px;
  overflow: hidden;
}

/* Main Content 영역 잡기*/
.main-content-area {
	flex: 1;
	padding: 25px;
	margin: 0 80px 0 240px; /* 사이드바 너비(200px) + 패딩 40px 정도 확보 */
	display: flex;
	flex-direction: column;
	/* color: #c5c8cc; */ /* 기본 텍스트 색상 설정 */
	color: #000000;
	/* 전체 배경 적용 */
	min-height: calc(100vh - 60px); /* 헤더를 제외한 나머지 높이 */
}
.main-content-area.main-loginForm {
	margin:0;
}
.app-container:has(#pdfTarget){
	margin: 0;
}
.main-content-area:has(#pdfTarget){
	margin: 0;
    max-width: 900px;
}
/* 흠? */
.app-container-wrapper {
	display: flex;
	flex-direction: column;
	min-height: 100vh; /* 전체 뷰포트 높이 차지 */
}

/* 특정 요소들에 대한 기본 스타일 */
p, br { /* 원본 코드의 p 태그와 br 태그가 스타일을 가지도록 추가 */
	color: #ffffff; /* p 태그의 글씨색을 흰색으로 설정 */
	margin-bottom: 10px; /* 줄 간격 조절 */
}

/* 링크 스타일 */
a {
	color: #7995ff; /* 링크 색상 */
	text-decoration: none !important; /* 밑줄 제거 */
	font-size: 16px;
	padding: 5px 0;
	display: inline-block; /* 패딩을 위해 블록 요소처럼 보이게 */
	transition: color 0.2s ease;
}

a:hover {
	color: #6070e0; /* 호버 시 색상 변경 */
	text-decoration: none !important; /* 호버 시 밑줄 추가 */
}
/* 브레드크럼 start */
.breadcrumb-item+.breadcrumb-item{
    display: flex;
    align-items: center;
}
.breadcrumb-item + .breadcrumb-item::before {
  content: ">" !important;
}

.breadcrumb .breadcrumb-item > a {
	color: #7995ff; /* 링크 색상 */
	text-decoration: none !important; /* 밑줄 제거 */
	font-size: 18px;
	padding: 2px 6px;
	display: inline-block; /* 패딩을 위해 블록 요소처럼 보이게 */
	border-radius:10px;
	transition: color 0.2s ease;
}

.breadcrumb .breadcrumb-item > a:hover {
	color: #fff;
    background-color: #0069d9;
    border-color: #0062cc;
	text-decoration: none !important; /* 호버 시 밑줄 추가 */
}
/* 브레드크럼 end */
/* 페이지 상단 해더 */
.content-title{
    display: inline-block;
	font-size: 40px;
	font-weight: bold;
}
/* 페이지 설명 */
.desc{	
    display: inline-block;
    margin:0;
    color:#000000;
}
/*필수 표기*/
.essential {
  position: relative;
}

.essential::before {
  content: "\f005"; /* Font Awesome solid star 아이콘 코드 */
  font-family: "Font Awesome 6 Free"; /* Font Awesome 폰트 지정 */
  font-weight: 900; /* solid 아이콘에 필요한 weight */
  color: #e74c3c; /* 빨간색 별 (원하는 색상으로 변경) */
  font-size: 0.8em; /* 텍스트보다 살짝 작게 */
  position: absolute;
  left: -2px;
  top: 50%;
  transform: translateY(-90%);
}
